  "dependencies": {},
  "devDependencies": {
    "@transferwise/eslint-config": "^5.0.3",
    "@types/node": "^13.7.7",
    "glob": "^7.1.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "release-to-github-with-changelog": "^1.2.2",
    "rimraf": "^3.0.2",
    "svgo": "^1.3.2",
    "svgson": "^4.0.0",
