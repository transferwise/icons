{
  "name": "@transferwise/icons",
  "version": "1.5.0",
  "description": "TransferWise SVG icons",
  "main": "index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rimraf lib build && mkdir lib && mkdir -p build/components",
    "build:react-components": "yarn run build-react-components",
    "build:format": "yarn run lint-fix",
    "test": "yarn run lint",
    "lint": "eslint './{scripts,build}/**/*.{ts,tsx}'",
    "lint-fix": "yarn run lint --fix",
    "check-release-to-github": "release-to-github-with-changelog-pre-release-checks",
    "release-to-github": "release-to-github-with-changelog",
    "build-react-components": "ts-node --project ./tsconfig.scripts.json ./scripts/build-react-components.ts"
  },
  "repository": {
    "type": "git",
    "fullname": "transferwise/icons",
    "url": "git+https://github.com/transferwise/icons.git"
  },
  "author": "TransferWise Ltd.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/transferwise/icons/issues"
  },
  "homepage": "https://github.com/transferwise/icons#readme",
  "dependencies": {},
  "devDependencies": {
    "@transferwise/eslint-config": "^5.0.3",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "glob": "^7.1.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "release-to-github-with-changelog": "^1.2.2",
    "rimraf": "^3.0.2",
    "svgo": "^1.3.2",
    "svgson": "^4.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
