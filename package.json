{
  "name": "@transferwise/icons",
  "version": "2.0.0-beta.4",
  "description": "TransferWise SVG icons",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "files": [
    "lib"
  ],
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rimraf lib build && mkdir lib && mkdir -p build/components && mkdir -p build/angular/components",
    "build:icon-components": "yarn run build-icon-components",
    "build:format": "yarn prettier --write",
    "build:rollup": "rollup -c",
    "rollup:watch": "rollup -cw",
    "test": "run-s test:*",
    "test:lint": "yarn lint",
    "test:check-release-to-github": "release-to-github-with-changelog-pre-release-checks",
    "lint": "eslint './scripts/**/*.{ts,tsx}'",
    "lint-fix": "yarn run lint --fix",
    "prettier": "prettier './build/**/*.{ts,tsx,js}'",
    "release-to-github": "release-to-github-with-changelog",
    "build-icon-components": "ts-node --project ./tsconfig.scripts.json ./scripts/build-icon-components.ts"
  },
  "repository": {
    "type": "git",
    "fullname": "transferwise/icons",
    "url": "git+https://github.com/transferwise/icons.git"
  },
  "author": "TransferWise Ltd.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/transferwise/icons/issues"
  },
  "homepage": "https://github.com/transferwise/icons#readme",
  "peerDependencies": {
    "react": "*",
    "angular": "*"
  },
  "devDependencies": {
    "@transferwise/eslint-config": "^5.0.3",
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "glob": "^7.1.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "release-to-github-with-changelog": "^1.2.3",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.0",
    "rollup-plugin-analyzer": "^3.2.2",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "svgo": "^1.3.2",
    "svgson": "^4.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "prettier": "@transferwise/eslint-config/.prettierrc.js"
}
