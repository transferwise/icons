{
  "name": "@transferwise/icons",
  "version": "1.5.0",
  "description": "TransferWise SVG icons",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "files": [
    "lib"
  ],
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rimraf lib",
    "build:react-components": "yarn run build-icon-components",
    "build:format": "yarn prettier ./src/**/*.{tsx,ts} --write",
    "build:rollup": "rollup -c",
    "rollup:watch": "rollup -cw",
    "test": "run-s test:*",
    "test:check-release-to-github": "release-to-github-with-changelog-pre-release-checks",
    "release-to-github": "release-to-github-with-changelog",
    "rename-icons": "ts-node --project ./tsconfig.scripts.json ./scripts/rename-files.ts",
    "build-icon-components": "ts-node --project ./tsconfig.scripts.json ./scripts/build-icon-components.ts"
  },
  "repository": {
    "type": "git",
    "fullname": "transferwise/icons",
    "url": "git+https://github.com/transferwise/icons.git"
  },
  "author": "TransferWise Ltd.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/transferwise/icons/issues"
  },
  "homepage": "https://github.com/transferwise/icons#readme",
  "devDependencies": {
    "@types/node": "^13.7.2",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.6",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "release-to-github-with-changelog": "^1.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-typescript2": "^0.26.0",
    "svgo": "^1.3.2",
    "svgson": "^4.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
